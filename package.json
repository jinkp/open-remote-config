{
  "name": "opencode-remote-config",
  "version": "0.5.0",
  "description": "OpenCode plugin to sync skills, agents and commands from Git repositories",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/softrestaurant-team/opencode-remote-config.git"
  },
  "homepage": "https://bitbucket.org/softrestaurant-team/opencode-remote-config#readme",
  "bugs": {
    "url": "https://bitbucket.org/softrestaurant-team/opencode-remote-config/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "opencode-remote-config-setup": "./dist/setup.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun && bun build src/setup.ts --outdir dist --target node",
    "dev": "bun build src/index.ts --outdir dist --target bun --watch",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "opencode",
    "plugin",
    "skills",
    "agents",
    "git",
    "sync"
  ],
  "author": "Jeroen Gordijn",
  "license": "MIT",
  "dependencies": {
    "gray-matter": "^4.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@opencode-ai/plugin": "^1.0.204",
    "@types/bun": "^1.1.14",
    "ajv": "^8.17.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@opencode-ai/plugin": ">=1.0.0"
  }
}
